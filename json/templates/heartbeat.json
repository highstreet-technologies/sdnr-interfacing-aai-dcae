{
  "event": {
    "commonEventHeader": {
      "domain": "@domain@",
      "eventId": "@eventId@",
      "eventName": "@domain@_@eventType@",
      "eventType": "@eventType@",
      "sequence": 0,
      "priority": "Low",
      "reportingEntityId": "",
      "reportingEntityName": "@controllerName@",
      "sourceId": "",
      "sourceName": "@controllerName@",
      "startEpochMicrosec": "@timestamp@",
      "lastEpochMicrosec": "@timestamp@",
      "version": 3.0
    },
    "heartbeatFields": {
      "heartbeatFieldsVersion": 1.0,
      "heartbeatInterval": 20,
      "additionalFields": [
        {
          "name": "eventTime",
          "value": "@eventTime@"
        }
      ]
    }
  }
}