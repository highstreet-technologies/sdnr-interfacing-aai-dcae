{
  "event": {
    "commonEventHeader": {
      "domain": "heartbeat",
      "eventId": "@eventId@",
      "eventType": "ControllerHeartbeat",
      "sequence": 0,
      "priority": "Low",
      "reportingEntityId": "@controllerId@",
      "reportingEntityName": "@controllerName@",
      "sourceId": "@controllerId@",
      "sourceName": "@controllerName@",
      "lastEpochMicrosec": "@timestamp@",
      "version": 2.0
    },
    "heartbeatFields": {
      "heartbeatFieldsVersion": 1.1,
      "heartbeatInterval": 20,
      "additionalFields": [
        {
          "name": "eventTime",
          "value": "@eventTime@"
        }
      ]
    }
  }
}